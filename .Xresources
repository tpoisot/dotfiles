! font
*.font: xft:Hack:size=9

URxvt*.cursorBlink: 1
URxvt.scrollBar: false
URxvt*depth: 32

Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.autohint: 0
Xft.rgba: rgb
Xft.hinting: 1
Xft.hinstyle: hintfull

Xcursor.theme: cursor-neutral

! xterm config
XTerm*termname: xterm-256color
XTerm*locale: true

#include ".schemes/x.xresources"
