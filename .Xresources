!font
*.faceName: xft:Hack:size=10

URxvt*.cursorBlink: 1
URxvt.scrollBar: false
URxvt*depth: 32

Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.autohint: 0
Xft.rgba: rgb
Xft.hinting: 1
Xft.hinstyle: hintfull

Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 22

! xterm config
XTerm*termname: xterm-256color
XTerm*locale: true
XTerm*highlightSelection: true

#include ".schemes/x.xresources"
